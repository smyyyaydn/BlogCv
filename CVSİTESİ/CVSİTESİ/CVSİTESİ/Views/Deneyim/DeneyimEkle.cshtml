
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form  method="post">
    <label></label>
    <input type="text" name="Başlık"  class="form-control" placeholder="Başlığı yazınız"/>
    <br />
    <input type="text" name="AltBaşlık"  class="form-control" placeholder="Alt Başlık"/>
    <br />
    <input type="text" name="Tarih"  class="form-control" placeholder="Çalışma Tarihi"/>
    <br />
    <textarea placeholder="Açıklamanızı giriniz" name="Açıklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

