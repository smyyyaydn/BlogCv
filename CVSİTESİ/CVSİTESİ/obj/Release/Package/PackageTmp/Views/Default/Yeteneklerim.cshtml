@using CVSİTESİ.Models.Entity
@model List<TblYeteneklerim>
<div class="resume-section-content">
    <h2 class="mb-5">Yeteneklerim</h2>
    <div class="subheading mb-3">Programlama Dilleri & Araçları</div>
    <ul class="list-inline dev-icons">
        <li class="list-inline-item"><i class="fab fa-html5"></i></li>
        <li class="list-inline-item"><i class="fab fa-css3-alt"></i></li>
        <li class="list-inline-item"><i class="fab fa-wordpress"></i></li>
        <li class="list-inline-item"><i class="fab fa-microsoft"></i></li>
        <li class="list-inline-item"><i class="fab fa-python"></i></li>
        <li class="list-inline-item"><i class="fab fa-unity"></i></li>
        <li class="list-inline-item"><i class="fa solid fa-database"></i></li>
        
       
    </ul>
    <div class="subheading mb-3">PROJELERİM</div>
    <ul class="fa-ul mb-0">
        @foreach (var x in Model)
        {
        <li>
            <span class="fa-li"><i class="fas fa-check"></i></span>
            @x.Yetenek
        </li>
        }
    </ul>
</div>